#include <bits/stdc++.h>
using namespace std;
int fac(int n)
{
    int x=1;
    for(int i=1;i<=n;i++)    
    {
        x=x*i;
    }
    return x;
}
int giv_fun(int x)
{
    int res;
    res=1-(x/fac(2))+((x*x)/fac(4))-((x*x*x)/fac(6))+((x*x*x*x)/fac(8))-((x*x*x*x*x)/fac(10));
    return res;
}
int main()
{
    int num;
    cout<<"putting x in the given function and the ans is "<<endl;
	cout<<giv_fun(num);


}
